{
  "name": "sails-angular-7",
  "private": true,
  "version": "0.0.0",
  "description": "Sails application with angular 7",
  "keywords": [],
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
